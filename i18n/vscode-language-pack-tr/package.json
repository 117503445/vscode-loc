{
	"name": "vscode-language-pack-tr",
	"displayName": "Turkish Language Pack for Visual Studio Code",
	"description": "Language pack extension for Turkish",
	"version": "1.31.4",
	"publisher": "MS-CEINTL",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-loc"
	},
	"license": "SEE MIT LICENSE IN LICENSE.md",
	"engines": {
		"vscode": "^1.31.0"
	},
	"icon": "languagepack.png",
	"categories": [
		"Language Packs"
	],
	"contributes": {
		"localizations": [
			{
				"languageId": "tr",
				"languageName": "Turkish",
				"localizedLanguageName": "Türkçe",
				"translations": [
					{
						"id": "vscode",
						"path": "./translations/main.i18n.json"
					}
				]
			}
		]
	},
	"scripts": {
		"update": "cd ../vscode && npm run update-localization-extension tr"
	}
}